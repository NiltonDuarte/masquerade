

import wishful_module_wifi_wmp as wmp
wm = wmp.WmpModule()
kwa = {'interface' : 'wlan0', "CSMA_CW" : 1, "CSMA_CW_MIN" :1, "CSMA_CW_MAX" : 16}
wm.set_parameter_lower_layer(**kwa)
wm.set_parameter_lower_layer(interface='wlan0', CSMA_CW=1, CSMA_CW_MIN=1, CSMA_CW_MAX=16)

